{
  "name": "gnetwork",
  "version": "3.10.0",
  "description": "WordPress Network Helper",
  "license": "GPL-3+",
  "homepage": "https://geminorum.ir/wordpress/gnetwork/",
  "author": {
    "name": "Nasser Rafie",
    "email": "contact@geminorum.ir",
    "url": "https://geminorum.ir"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/geminorum/gnetwork.git"
  },
  "bugs": {
    "url": "https://github.com/geminorum/gnetwork/issues"
  },
  "main": "gulpfile.js",
  "scripts": {
    "gulp": "./node_modules/gulp/bin/gulp.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "md .build && cd .build && git clone https://github.com/geminorum/gnetwork . && composer install --no-dev --optimize-autoloader --prefer-dist -v && yarn install && gulp build",
    "ready": "gulp ready",
    "refresh": "gulp dev:styles",
    "locale:old": "gulp pot",
    "locale:pot": "wp i18n make-pot . --include=includes,assets/blocks,gnetwork.php --exclude=assets/blocks/**/src,.temp --file-comment='' --headers='{\"Last-Translator\":\"NULL\",\"Language-Team\":\"NULL\",\"Report-Msgid-Bugs-To\":\"https://github.com/geminorum/gnetwork/issues\"}'",
    "locale:json": "wp i18n make-json languages --no-purge",
    "locale:core": "wp i18n make-json assets/locale/core --no-purge",
    "watch": "gulp watch:styles",
    "patch": "gulp bump --patch",
    "minor": "gulp bump",
    "changes": "echo ### > CHANGES.temp && git changelog >> CHANGES.temp && (echo. & echo.) >> CHANGES.temp && type CHANGES.md >> CHANGES.temp && type CHANGES.temp > CHANGES.md && del CHANGES.temp",
    "release": "gulp github:package && start https://github.com/geminorum/gnetwork/releases",
    "home": "start https://github.com/geminorum/gnetwork/",
    "issues": "start https://github.com/geminorum/gnetwork/issues"
  },
  "devDependencies": {
    "@wordpress/browserslist-config": "^2.6.0",
    "autoprefixer": "^9.7.3",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.1.3",
    "gulp-changed-in-place": "^2.3.0",
    "gulp-checktextdomain": "^2.2.1",
    "gulp-debug": "^4.0.0",
    "gulp-exclude-gitignore": "^1.2.0",
    "gulp-github-release": "^1.2.1",
    "gulp-header": "^2.0.9",
    "gulp-if": "^3.0.0",
    "gulp-livereload": "^4.0.2",
    "gulp-load-plugins": "^2.0.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-size": "^3.0.0",
    "gulp-smushit": "^1.2.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-svgmin": "^2.2.0",
    "gulp-tinypng-extended": "^2.0.2",
    "gulp-uglify": "^3.0.2",
    "gulp-wp-pot": "^2.3.6",
    "gulp-zip": "^5.0.1",
    "js-yaml": "^3.13.1",
    "multipipe": "^3.0.1",
    "parse-changelog": "^1.0.2",
    "prettyjson": "^1.2.1",
    "publish-release": "^1.6.1",
    "rtlcss": "^2.4.1",
    "semistandard": "^14.2.0",
    "stylelint": "^12.0.1",
    "stylelint-config-wordpress": "^16.0.0",
    "xtend": "^4.0.2"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "semistandard": {
    "ignore": [
      "/assets/js/vendor/",
      "*.min.js"
    ],
    "globals": [
      "gNetwork",
      "autosize",
      "jQuery",
      "ajaxurl"
    ]
  },
  "readme": "ERROR: No README data found!",
  "private": true
}
